@using UrlRouting.Areas.Sales.Models;
@model Product[]
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>Routing</title>
</head>
<body>
    <table style="border: 1px solid black;">
        <tr>
            <th>Name</th>
            <th>Quantity</th>
            <th>Price</th>
        </tr>
        @foreach (Product p in Model)
        {
            <tr>
                <td>@p.Name</td>
                <td>@p.Quantity</td>
                <td>@p.Price</td>
            </tr>
        }
    </table>
</body>
</html>